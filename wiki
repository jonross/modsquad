# vim:ft=python

import sys
import tavolino as tl

tl.getargs(tl.noargs)

input = tl.getrows()
rows = list(input)
sample = tl.transpose(rows[:20])

for i in range(len(sample)):
    typ, convert = tl.guess_type(sample[i])
    format = tl.make_format(typ, True)
    if typ in [int, float]:
        for row in rows:
            row[i] = " " + format(convert(row[i]))

for row in rows:
    print "| " + " | ".join(row) + " |"



